{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-ts-starter",
  "description": "Vue2 TypeScript Startar project for Vite.",
  "version": "1.2.1",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vue2-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vue2-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vue2-ts-starter/issues"
  },
  "engines": {
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/vue2-helpers": "^2.1.4",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "axios": "^1.3.5",
    "bootstrap-vue": "^2.23.1",
    "highcharts": "^10.3.3",
    "highcharts-vue": "^1.4.0",
    "pinia": "^2.0.34",
    "socket.io-client": "^4.6.1",
    "sweetalert2": "^11.7.3",
    "vue": "^2.7.14",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.6.5",
    "vue-slick": "^1.2.0",
    "vue-slick-carousel": "^1.0.6",
    "vue-svg-loader": "^0.16.0",
    "vue-sweetalert2": "^5.0.5",
    "vue2-datepicker": "^3.11.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.8",
    "@types/vue2-datepicker": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^1.3.4",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vitest": "^0.29.7",
    "vue-eslint-parser": "^9.1.0",
    "vue-template-compiler": "^2.7.1",
    "vue-tsc": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,toml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
